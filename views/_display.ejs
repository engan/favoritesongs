<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Bandname">Artist / Group</label>
  <div class="col-sm-8">
    <input readonly class="form-control" id="Bandname" value="<%= model.Bandname %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="SongTitle">Favourite song</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="SongTitle" value="<%= model.SongTitle %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Year">Published year</label>
  <div class="col-sm-10">
    <input readonly class="form-control" id="Year" value="<%= model.Year %>" />
  </div>
</div>
